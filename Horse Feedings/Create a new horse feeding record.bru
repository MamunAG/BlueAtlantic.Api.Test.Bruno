meta {
  name: Create a new horse feeding record
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/api/v1/horse-feedings
  body: json
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

body:json {
  {
    "horseId": "",
    "feedTypeId": "",
    "supplementId": "",
    "quantityKg": "",
    "feedingTime": "",
    "recordedByUserId": ""
  }
}

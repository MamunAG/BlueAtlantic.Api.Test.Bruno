meta {
  name: Create a new horse
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/api/v1/horses
  body: json
  auth: bearer
}

auth:bearer {
  token: {{token}}
}

body:json {
  {
    "name": "",
    "breedId": "",
    "dateOfBirth": "",
    "ageYears": "",
    "assignedRiderId": "",
    "status": "",
    "gender": "",
    "passportNumber": "",
    "lastSessionAt": "",
    "mediaAssetsId": "",
    "notes": ""
  }
}
